{
  "name": "fitness-uno",
  "version": "1.0.0",
  "description": "fitness uno",
  "main": "_dist/index.js",
  "scripts": {
    "clean": "rimraf _dist/*",
    "copy-assets": "node _dist/tools/copyAssets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch frontend/src --watch backend --watch tools -e css,ts,tsx,jsx,js,json --exec npm run dev:start",
    "start": "node .",
    "tsc": "tsc -p ./tsconfig.json",
    "frontend-build": "cd frontend && react-scripts build",
    "frontend-start": "cd frontend && react-scripts build",
    "build": "npm-run-all clean tsc frontend-build && npm run copy-assets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jromeem/fitness-uno.git"
  },
  "author": "jerome and russel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jromeem/fitness-uno/issues"
  },
  "homepage": "https://github.com/jromeem/fitness-uno#readme",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/express": "^4.17.3",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/request": "^2.48.4",
    "@types/tape": "^4.2.34",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/shelljs": "^0.8.7",
    "fs-extra": "^9.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "request": "^2.81.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "tape": "^4.7.0",
    "ts-node": "^8.8.1"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "react"
  ],
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
